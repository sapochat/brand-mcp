{
  "name": "brand-safety-mcp",
  "version": "2.1.0",
  "description": "Enterprise-grade Model Context Protocol server for AI content brand safety and compliance",
  "main": "dist/main.js",
  "type": "module",
  "bin": {
    "brand-mcp": "dist/cli/index.js"
  },
  "author": "",
  "license": "MIT",
  "homepage": "https://github.com/sapochat/brand-mcp#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sapochat/brand-mcp.git"
  },
  "bugs": {
    "url": "https://github.com/sapochat/brand-mcp/issues"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "brand-safety",
    "brand-compliance",
    "ai-safety",
    "content-moderation",
    "llm",
    "typescript",
    "enterprise"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/main.js",
    "dev": "tsc-watch --onSuccess \"node dist/main.js\"",
    "cli": "node dist/cli/index.js",
    "test": "npm run test:unit",
    "demo": "tsc && node dist/examples/brandComplianceClient.js",
    "sdk-demo": "tsc && node dist/examples/sdkExampleClient.js",
    "enhanced-test": "node dist/examples/enhancedTestClient.js",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "test:unit": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "audit": "npm audit",
    "audit:fix": "npm audit fix"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.25.3",
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "zod": "^3.25.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@jest/globals": "^30.0.5",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.9.0",
    "@typescript-eslint/eslint-plugin": "^8.41.0",
    "@typescript-eslint/parser": "^8.41.0",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^30.0.5",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.1",
    "tsc-watch": "^6.0.4",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.54.0"
  }
}
