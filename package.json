{
  "name": "brand-safety-mcp",
  "version": "1.0.0",
  "description": "Enterprise-grade Model Context Protocol server for AI content brand safety and compliance",
  "main": "dist/index.js",
  "type": "module",
  "author": "",
  "license": "MIT",
  "homepage": "https://github.com/sapochat/brand-mcp#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sapochat/brand-mcp.git"
  },
  "bugs": {
    "url": "https://github.com/sapochat/brand-mcp/issues"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "brand-safety",
    "brand-compliance",
    "ai-safety",
    "content-moderation",
    "llm",
    "typescript",
    "enterprise"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsc-watch --onSuccess \"node dist/index.js\"",
    "test": "tsc && node dist/client/testClient.js",
    "demo": "tsc && node dist/client/brandComplianceClient.js",
    "sdk-demo": "tsc && node dist/client/sdkExampleClient.js",
    "enhanced-test": "node dist/client/enhancedTestClient.js",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "test:unit": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "audit": "npm audit",
    "audit:fix": "npm audit fix"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.11.0",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "@jest/globals": "^30.0.5",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.9.0",
    "@typescript-eslint/eslint-plugin": "^8.41.0",
    "@typescript-eslint/parser": "^8.41.0",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^30.0.5",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.1",
    "tsc-watch": "^6.0.4",
    "typescript": "^5.3.3"
  }
}
